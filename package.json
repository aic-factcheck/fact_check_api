  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "unit-test": "jest \"--testRegex='.xspec.ts$'\" --runInBand --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --runInBand --config ./test/jest-e2e.json --coverage",
    "prepare": "ts-patch install && husky install",
    "docker:for-fe": "docker-compose -f docker-compose.dev.yaml up",
    "docker:dev": "docker-compose up -d && npm run start:dev",
    "docker:debug": "docker-compose up -d && npm run start:debug",
    "docker:down": "docker-compose down",
    "docker:prod": "docker-compose up -d && npm run start:prod",
    "docker:test": "docker-compose up -d && npm run test:e2e",
    "docs:uml": "java -jar /home/rasto/.vscode-server/extensions/jebbs.plantuml-2.17.5/plantuml.jar './uml_docs/*.puml' -o ./output"
  },
